	LCD_WR_DATA(0x08);
	LCD_WR_DATA(0x0F);
	LCD_WR_DATA(0x0C);
	LCD_WR_DATA(0x31);
	LCD_WR_DATA(0x36);
	LCD_WR_DATA(0x0F);

	//EXIT SLEEP
	LCD_Write_COM(0x11);
	HAL_Delay(120);

	//TURN ON DISPLAY
	LCD_Write_COM(0x29);
}

void setXY(uint32_t x1, uint32_t y1, uint32_t x2, uint32_t y2){
	if (orient==LANDSCAPE)
	{
		swap(uint32_t, x1, y1);
		swap(uint32_t, x2, y2)
		y1=SIZE_Y-y1;
		y2=SIZE_Y-y2;
		//x1=SIZE_X-x1;
		//x2=SIZE_X-x2;
		swap(uint32_t, y1, y2);
	}
	LCD_Write_COM(0x2A);
	LCD_WR_DATA(x1>>8);
	LCD_WR_DATA(x1);
	LCD_WR_DATA(x2>>8);
	LCD_WR_DATA(x2);

	LCD_Write_COM(0x2B);
	LCD_WR_DATA(y1>>8);
	LCD_WR_DATA(y1);
	LCD_WR_DATA(y2>>8);
	LCD_WR_DATA(y2);	
	LCD_Write_COM(0x2c);	
	
}
void clrXY(){
	if (orient==PORTRAIT)
		setXY(0,0,SIZE_X,SIZE_Y);
	else
		setXY(0,0,SIZE_Y,SIZE_X);
}
/*
*	Ham chon mau sac theo dinh dang RGB
* r: gia tri mau do 
* g: gia tri mau xanh Green
* b: gia tri mau xanh Blue
*/
void lcd_set_colorRGB(uint8_t r, uint8_t g, uint8_t b){
	fch=((r&248)|g>>5);
	fcl=((g&28)<<3|b>>3);
}
/*
*	Ham chon mau sac theo dinh dang 24bit
* color: gia tri mau sac theo dinh dang 24 bit 
*/
void lcd_set_color16bit(uint32_t color){
	fch=(uint8_t)(color>>8);
	fcl=(uint8_t)(color & 0xFF);
}

/*
*	Ham ve hinh tron rong
* x,y: Toa do tam
* radius: Ban kinh hinh tron
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi ve
*/
void lcd_draw_circle_outline(int x, int y, int radius){
	int f = 1 - radius;
	int ddF_x = 1;
	int ddF_y = -2 * radius;
	int x1 = 0;
	int y1 = radius;
 
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;;
	setXY(x, y + radius, x, y + radius);
	LCD_Write_DATA(fch,fcl);
	setXY(x, y - radius, x, y - radius);
	LCD_Write_DATA(fch,fcl);
	setXY(x + radius, y, x + radius, y);
	LCD_Write_DATA(fch,fcl);
	setXY(x - radius, y, x - radius, y);
	LCD_Write_DATA(fch,fcl);
 
	while(x1 < y1)
	{
		if(f >= 0) 
		{
			y1--;
			ddF_y += 2;
			f += ddF_y;
		}
		x1++;
		ddF_x += 2;
		f += ddF_x;    
		setXY(x + x1, y + y1, x + x1, y + y1);
		LCD_Write_DATA(fch,fcl);
		setXY(x - x1, y + y1, x - x1, y + y1);
		LCD_Write_DATA(fch,fcl);
		setXY(x + x1, y - y1, x + x1, y - y1);
		LCD_Write_DATA(fch,fcl);
		setXY(x - x1, y - y1, x - x1, y - y1);
		LCD_Write_DATA(fch,fcl);
		setXY(x + y1, y + x1, x + y1, y + x1);
		LCD_Write_DATA(fch,fcl);
		setXY(x - y1, y + x1, x - y1, y + x1);
		LCD_Write_DATA(fch,fcl);
		setXY(x + y1, y - x1, x + y1, y - x1);
		LCD_Write_DATA(fch,fcl);
		setXY(x - y1, y - x1, x - y1, y - x1);
		LCD_Write_DATA(fch,fcl);
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}


/*
*	Ham ve hinh tron duoc phu kin mau
* x,y: Toa do tam
* radius: Ban kinh hinh tron
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi ve
*/
void lcd_draw_circle_fill(int x, int y, int radius){
int y1,x1;	
	for( y1=-radius; y1<=0; y1++) 
		for( x1=-radius; x1<=0; x1++)
			if(x1*x1+y1*y1 <= radius*radius) 
			{
				lcd_draw_Hline(x+x1, y+y1, 2*(-x1));
				lcd_draw_Hline(x+x1, y-y1, 2*(-x1));
				break;
			}
}



/*
*	Ham ve nua hinh tron duoc phu kin mau
* x,y: Toa do tam
* radius: Ban kinh hinh tron
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi ve
*/
void lcd_draw_halfcircle_fill(int x, int y, int radius){
int y1,x1;	
	for( y1=-radius; y1<=0; y1++) 
		for( x1=-radius; x1<=0; x1++)
			if(x1*x1+y1*y1 <= radius*radius) 
			{
				lcd_draw_Hline(x+x1, y+y1, 2*(-x1));
				break;
			}
}
/*
*	Ham chon mau 1 pixel
* color: Mau pixel theo dinh dang 16 bit
*	Goi ham chon toa do setXY cho pixel truoc khi to mau
*/
void lcd_set_pixel(uint32_t color){
	LCD_Write_DATA((color>>8),(color&0xFF));	// rrrrrggggggbbbbb
}

/*
*	Ham to mau 1 pixel
* x,y: toa do cua pixel
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi to mau
*/
void lcd_draw_pixel(int x, int y){
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	setXY(x, y, x, y);
	lcd_set_pixel((fch<<8)|fcl);
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}
/*
*	Ham ve mot duong thang
* x1,y1: toa do diem dau cua duong thang
* x2,y2: toa do diem cuoi
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi to mau
*/
void lcd_draw_line(int x1, int y1, int x2, int y2){
	if (y1==y2)
		lcd_draw_Hline(x1, y1, x2-x1);
	else if (x1==x2)
		lcd_draw_Vline(x1, y1, y2-y1);
	else
	{
		unsigned int	dx = (x2 > x1 ? x2 - x1 : x1 - x2);
		short			xstep =  x2 > x1 ? 1 : -1;
		unsigned int	dy = (y2 > y1 ? y2 - y1 : y1 - y2);
		short			ystep =  y2 > y1 ? 1 : -1;
		int				col = x1, row = y1;

		LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
		if (dx < dy)
		{
			int t = - (dy >> 1);
			while (true)
			{
				setXY (col, row, col, row);
				LCD_Write_DATA (fch, fcl);
				if (row == y2)
					return;
				row += ystep;
				t += dx;
				if (t >= 0)
				{
					col += xstep;
					t   -= dy;
				}
			} 
		}
		else
		{
			int t = - (dx >> 1);
			while (true)
			{
				setXY (col, row, col, row);
				LCD_Write_DATA (fch, fcl);
				if (col == x2)
					return;
				col += xstep;
				t += dy;
				if (t >= 0)
				{
					row += ystep;
					t   -= dx;
				}
			} 
		}
		LCD_CS_PORT->BSRR = LCD_CS_PIN;
	}
	clrXY();
}


/*
*	Ham ve mot hinh vuong phu kin mau
* x1,y1: toa do diem dau cua hinh vuong
* dai: kich thuoc theo truc x
* rong: kich thuoc theo truc y
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi to mau
*/
void lcd_draw_square_fill(int x1,int y1,int dai,int rong){
 int i=0;
	int x2,y2;
	x2 = x1 + dai;
	y2 = y1 + rong;
	for(i=0;i<y2-y1;i++){
		lcd_draw_line(x1,y1+i,x2,y1+i);
	}
}

/*
*	Ham ve mot hinh vuong goc bo tron phu kin mau
* x1,y1: toa do diem dau cua hinh vuong
* dai: kich thuoc theo truc x
* rong: kich thuoc theo truc y
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi to mau
*/
void lcd_draw_square_curve_fill(int x1,int y1,int dai,int rong){
 int i=0,x2,y2,tam_rong;
	x2=x1+dai;
	y2=y1+rong;
	tam_rong=(y2-y1)/2;
	lcd_draw_line(x1,y1,x2,y1);
	lcd_draw_line(x1-1,y1+1,x2+1,y1+1);
	lcd_draw_line(x1-2,y1+2,x2+2,y1+2);
	for(i=3;i<(y2-2)-y1;i++){
		lcd_draw_line(x1-3,y1+i,x2+3,y1+i);
	}
	lcd_draw_line(x1-2,y2-2,x2+2,y2-2);
	lcd_draw_line(x1-1,y2-1,x2+1,y2-1);
	lcd_draw_line(x1,y2,x2,y2);
}
/*
*	Ham ve mot hinh vuong goc bo tron rong
* x1,y1: toa do diem dau cua hinh vuong
* dai: kich thuoc theo truc x
* rong: kich thuoc theo truc y
* color: Mau sac theo dinh dang 16bit
*/
void lcd_draw_square_curve_outline(int x1,int y1,int dai,int rong,uint32_t color){
 int i=0,x2,y2,tam_rong;
	lcd_set_color16bit(color);
	x2=x1+dai;
	y2=y1+rong;
	tam_rong=(y2-y1)/2;
	lcd_draw_line(x1,y1,x2,y1);
	lcd_draw_pixel(x1-1,y1+1);lcd_draw_pixel(x2+1,y1+1);
	lcd_draw_pixel(x1-2,y1+2);lcd_draw_pixel(x2+2,y1+2);
	for(i=3;i<(y2-2)-y1;i++){
		lcd_draw_pixel(x1-3,y1+i);lcd_draw_pixel(x2+3,y1+i);
	}
	lcd_draw_pixel(x1-2,y2-2);lcd_draw_pixel(x2+2,y2-2);
	lcd_draw_pixel(x1-1,y2-1);lcd_draw_pixel(x2+1,y2-1);
	lcd_draw_line(x1,y2,x2,y2);
}
/*
*	Ham ve mot hinh vuong rong
* x1,y1: toa do diem dau cua hinh vuong
* dai: kich thuoc theo truc x
* rong: kich thuoc theo truc y
* color: Mau sac theo dinh dang 16bit
*/
void lcd_draw_square_outline(int x1,int y1,int dai,int rong,uint32_t color){
 int x2,y2;
	lcd_set_color16bit(color);
	x2=x1+dai;
	y2=y1+rong;
	lcd_draw_Hline(x1,y1,dai);
	lcd_draw_Vline(x1,y1,rong);
	
	lcd_draw_Hline(x1,y2,dai);	
	lcd_draw_Vline(x2,y1,rong);
	
}
/*
*	Ham phu mau toan man hinh
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi to mau
*/
void lcd_screen_fill(void){
	int i=0;
	int line = 0;
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	
	for(line = 0;line < 240;line = line +2)
	{
		setXY(0, line, 320, line);
		for( i=0; i<320; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	for(line = 239;line > 0;line = line-2)
	{
		setXY(0, line, 320, line);
		for( i=0; i<320; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}

/*
*	Ham ve duong thang theo truc x
* x,y: toa do diem dau cua duong thang
* l: chieu dai duong thang
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi ve duong thang
*/
void lcd_draw_Hline(int x, int y, int l){
	int i=0;
	
	if (l<0)
	{
		l = -l;
		x -= l;
	}
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	setXY(x, y, x+l, y);	
	for ( i=0; i<l+1; i++)
	{
		LCD_Write_DATA(fch, fcl);
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}

/*
*	Ham ve duong thang theo truc y
* x,y: toa do diem dau cua duong thang
* l: chieu dai duong thang
*	Goi ham chon mau lcd_set_colorRGB hoac lcd_set_color16bit truoc khi ve duong thang
*/
void lcd_draw_Vline(int x, int y, int l){
	int i=0;
	
	if (l<0)
	{
		l = -l;
		y -= l;
	}
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	setXY(x, y, x, y+l);
	if (display_transfer_mode == 16)
	{
		LCD_RS_PORT->BSRR = LCD_RS_PIN;
	//	_fast_fill_16(fch,fcl,l);
	}
	else if ((display_transfer_mode==8) && (fch==fcl))
	{
		LCD_RS_PORT->BSRR = LCD_RS_PIN;
	//	_fast_fill_8(fch,l);
	}
	else
	{
		for ( i=0; i<l+1; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}



/*
*	Ham ve in mot ky tu len man hinh
* c : ky tu
* x,y: toa do diem dau cua duong thang
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau truoc khi in ky tu
* Goi ham setFont de cai dat font cho ky tu truoc khi in ky tu
*/
void printChar(u8 c, int x, int y){
	uint8_t i,ch;
	int zz=0;
	uint32_t j;
	uint32_t temp; 
	transparent = 0;
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
  
	
	if(cfont.y_size < 8)
	{
		temp=((c-cfont.offset)*((cfont.x_size/8)*(8)))+4;
				
		for(j=0;j<((cfont.x_size/8)*(8));j+=(cfont.x_size/8))
		{
			setXY(x,y+(j/(cfont.x_size/8)),x+cfont.x_size-1,y+(j/(cfont.x_size/8)));
			for ( zz=(cfont.x_size/8)-1; zz>=0; zz--)
			{
				ch=cfont.font[temp+zz];
				for(i=8-cfont.y_size;i<8;i++)
				{   
					if((ch&(1<<i))!=0)   
					{
						lcd_set_pixel((fch<<8)|fcl);
					} 
					else
					{
						lcd_set_pixel((bch<<8)|bcl);
					}   
				}
			}
			temp+=(cfont.x_size/8);
		}
	}
	else
	{	
		if(transparent)
		{
			if (orient==PORTRAIT)
			{
				setXY(x,y,x+cfont.x_size-1,y+cfont.y_size-1);
			
				temp=((c-cfont.offset)*((cfont.x_size/8)*cfont.y_size))+4;
				for(j=0;j<((cfont.x_size/8)*cfont.y_size);j++)
				{
					ch=cfont.font[temp];
					for(i=0;i<8;i++)
					{   
						if((ch&(1<<(7-i)))!=0)   
						{
							lcd_set_pixel((fch<<8)|fcl);
						} 
						else
						{
							lcd_set_pixel((bch<<8)|bcl);
						}   
					}
					temp++;
				}
			}
			else
			{
				temp=((c-cfont.offset)*((cfont.x_size/8)*cfont.y_size))+4;			
				for(j=0;j<((cfont.x_size/8)*cfont.y_size);j+=(cfont.x_size/8))
				{
					setXY(x,y+(j/(cfont.x_size/8)),x+cfont.x_size-1,y+(j/(cfont.x_size/8)));
					for ( zz=(cfont.x_size/8)-1; zz>=0; zz--)
					{
						ch=cfont.font[temp+zz];
						for(i=0;i<8;i++)
						{   
							if((ch&(1<<i))!=0)   
							{
								lcd_set_pixel((fch<<8)|fcl);
							} 
							else
							{
								lcd_set_pixel((bch<<8)|bcl);
							}   
						}
					}
					temp+=(cfont.x_size/8);
				}
			}
		}
		else
		{
			temp=((c-cfont.offset)*((cfont.x_size/8)*cfont.y_size))+4;
			for(j=0;j<cfont.y_size;j++) 
			{
				for ( zz=0; zz<(cfont.x_size/8); zz++)
				{
					ch=cfont.font[temp+zz]; 
					for(i=0;i<8;i++)
					{ 
					
						if((ch&(1<<(7-i)))!=0)   
						{
							setXY(x+i+(zz*8),y+j,x+i+(zz*8)+1,y+j+1);
							lcd_set_pixel((fch<<8)|fcl);
						} 
					}
				}
				temp+=(cfont.x_size/8);
			}
		}
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}
/*
*	Ham ve in mot ky tu len man hinh
* c : ky tu
* x,y: toa do diem dau cua duong thang
* deg : Goc quay ky tu
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau truoc khi in ky tu
* Goi ham setFont de cai dat font cho ky tu truoc khi in ky tu
*/
void rotateChar(u8 c, int x, int y, int pos, int deg){
	u8 i,j,ch;
	u32 temp; 
	int newx,newy,zz;
	char radian;
	radian=deg*0.0175;  

	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;

	temp=((c-cfont.offset)*((cfont.x_size/8)*cfont.y_size))+4;
	for(j=0;j<cfont.y_size;j++) 
	{
		for ( zz=0; zz<(cfont.x_size/8); zz++)
		{
			ch=cfont.font[temp+zz]; 
			for(i=0;i<8;i++)
			{   
				newx=x+(((i+(zz*8)+(pos*cfont.x_size))*cos(radian))-((j)*sin(radian)));
				newy=y+(((j)*cos(radian))+((i+(zz*8)+(pos*cfont.x_size))*sin(radian)));

				setXY(newx,newy,newx+1,newy+1);
				
				if((ch&(1<<(7-i)))!=0)   
				{
					lcd_set_pixel((fch<<8)|fcl);
				} 
				else  
				{
					if (!transparent)
						lcd_set_pixel((bch<<8)|bcl);
				}   
			}
		}
		temp+=(cfont.x_size/8);
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}
/*
*	Ham ve in mot chuoi ky tu len man hinh
* *st : chuoi ky tu
* x,y: toa do diem dau cua duong thang
* deg : Goc quay ky tu
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau truoc khi in ky tu
* Goi ham setFont de cai dat font cho ky tu truoc khi in ky tu
*/
void lcd_print_text(char *st, int x, int y, int deg){
	int stl, i;

	stl = strlen(st);

	if(orient==PORTRAIT)
	{
	if (x==RIGHT)
		x=(disp_x_size+1)-(stl*cfont.x_size);
	if (x==CENTER)
		x=((disp_x_size+1)-(stl*cfont.x_size))/2;
	}
	else
	{
	if (x==RIGHT)
		x=(disp_y_size+1)-(stl*cfont.x_size);
	if (x==CENTER)
		x=((disp_y_size+1)-(stl*cfont.x_size))/2;
	}

	for (i=0; i<stl; i++)
		if (deg==0)
			printChar(*st++, x + (i*(cfont.y_size)), y);
		else
			rotateChar(*st++, x, y, i, deg);
}




/*
*	Ham ve bieu tuong enter rong
* x,y: toa do trung tam bieu tuong
* size : kich thuoc do day cua bieu tuong
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau truoc khi in ky tu
*/
void lcd_draw_enter_outline(int x,int y,int size){
	x= x+size;
	y= y -size;
	size = size*2;
	
	lcd_draw_line(x,y,x,y+size);
	lcd_draw_line(x,y+size,x-size,y+size);
	lcd_draw_line(x,y,x-size/2,y);
	lcd_draw_line(x-size/2,y,x-size/2,y+size/2);
	lcd_draw_line(x-size/2,y+size/2,x-size,y+size/2);
	lcd_draw_line(x-size,y+size/2,x-size,y+size/4);
	lcd_draw_line(x-size,y+size/4,x-size-size/2,y+size-size/4);
	lcd_draw_line(x-size,y+size,x-size,y+size+size/4);
	lcd_draw_line(x-size,y+size+size/4,x-size-size/2,y+size-size/4);	
}

/*
*	Ham ve bieu tuong enter phu kin mau
* x,y: toa do trung tam bieu tuong
* size : kich thuoc do day cua bieu tuong
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau truoc khi in ky tu
*/
void lcd_draw_enter_fill(int x,int y,int size){
	int x0,y0;
	int x1,y1;
	int x2,y2;
	int i=0;
	int line = 0;
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	
	for(line = y-size;line < y+size;line++)
	{
		setXY(x, line, x+size, line);
		for( i=0; i<size; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	for(line = y;line < y+size;line++)
	{
		setXY(x-size, line, x, line);
		for( i=0; i<size; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	
	lcd_draw_triangle_fill(x-(3*size/2)+5,y+size/2,size,size*2,0);
	
	clrXY();
}

/*
*	Ham ve bieu tuong exit phu kin mau
* x,y: toa do trung tam bieu tuong
* size_r : Ban kinh duong tron bao ngoai
* color_inside: mau sac bieu tuong ben trong theo dinh dang 16 bit
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau nen cho bieu tuong
*/
void lcd_draw_exit_fill(int x,int y,int size_r,uint32_t color_inside){	
	int i;
	int x1,y1,x2,y2;
	int y_tmp;
	int thick = size_r/3;
	lcd_draw_circle_fill(x,y,size_r);
	size_r = size_r/2;
	lcd_set_color16bit(color_inside);
	
	x1 = x-size_r;
	x2 = x+size_r - thick;
	y1= y-size_r;
	y2 = y+size_r;
	
	for(i = 0;i<thick;i++)	lcd_draw_line(x1+i,y1,x2+i,y2);	
	for(i = 0;i<thick;i++)	lcd_draw_line(x1+i,y2,x2+i,y1);
}
/*
*	Ham ve bieu tuong return phu kin mau
* x,y: toa do trung tam bieu tuong
* size_r : Ban kinh duong tron bao ngoai
* color_inside: mau sac bieu tuong ben trong theo dinh dang 16 bit
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau nen cho bieu tuong
*/
void lcd_draw_return_fill(int x,int y,int size_r,uint32_t color_inside){	
	int i;
	int x1,y1,x2,y2;
	int y_tmp;
	int thick = size_r/3;
	int line;
	
	lcd_draw_circle_fill(x,y,size_r);
	size_r = size_r/2;
	lcd_set_color16bit(color_inside);
	x1 = x+thick/2;
	y1 = y+thick/2;
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	
	for(line = y1-thick;line < y1+thick;line++)
	{
		setXY(x1, line, x1+thick, line);
		for( i=0; i<thick; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	for(line = y1;line < y1+thick;line++)
	{
		setXY(x1-thick, line, x1, line);
		for( i=0; i<thick; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	for(line = y1-2*thick;line < y1-thick;line++)
	{
		setXY(x1-2*thick, line, x1+thick, line);
		for( i=0; i<3*thick; i++)
		{
			LCD_Write_DATA(fch, fcl);
		}		
	}
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	
	lcd_draw_triangle_fill(x1-thick,y1+thick/2,thick,thick*2,0);
	
	clrXY();
}

/*
*	Ham ve bieu tuong return phu kin mau
* x,y: toa do trung tam bieu tuong
* size_r : Ban kinh duong tron bao ngoai
* color_inside: mau sac bieu tuong ben trong theo dinh dang 16 bit
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau nen cho bieu tuong
*/
void lcd_draw_power_fill(int x,int y,int size_r,uint32_t color_inside){	
	int i;
	int x1,y1,x2,y2;
	int y_tmp;
	int thick = size_r/3;
	int line;
	uint8_t last_fch;
	uint8_t	last_fcl;
	
	last_fch = fch;
	last_fcl = fcl;
	
	lcd_draw_circle_fill(x,y,size_r);
	size_r = size_r-thick;
	lcd_set_color16bit(color_inside);
	for(i=0;i<thick/2;i++)	lcd_draw_circle_outline(x,y,size_r-i);	
	lcd_draw_square_fill(x-thick/3,y-size_r-thick/2,2*thick/3,size_r);
	clrXY();
}

/*
*	Ham ve bieu tuong tam giac phu kin mau
* x,y: toa do trung tam bieu tuong
* size_x : kich thuoc theo truc X
* size_y : kich thuoc theo truc Y
* deg: Goc quay cua bieu tuong
*	Goi ham lcd_set_colorRGB hoac lcd_set_color16bit de chon mau nen cho bieu tuong
*/
void lcd_draw_triangle_fill(int x,int y,int size_x,int size_y,int deg)
{
	int x0,y0,x1,y1,x2,y2;
	int x_tmp,y_tmp;
	int i,j;
	
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	if(deg == 0)
	{		
		x1 = x - size_x/2;
		x2 = x + size_x/2;
		y0 = y;
		y1 = y - size_y/2;
		y2 = y + size_y/2;
		
		for(i=0;i<size_x;i++)
		{			
			setXY(x1+i,y-(size_y*i/size_x)/2,x1+i,y+(size_y*i/size_x)/2);
			for(j=0;j<((i*size_y/size_x)+1);j++)
			{
				LCD_Write_DATA(fch,fcl);
			}		
		}	
	}
	else if(deg == 180)
	{
		x1 = x - size_x/2;
		x2 = x + size_x/2;
		y0 = y;
		y1 = y - size_y/2;
		y2 = y + size_y/2;
		
		for(i=0;i<size_x;i++)
		{			
			setXY(x1+i,y-(size_y*(size_x-i)/size_x)/2,x1+i,y+(size_y*(size_x-i)/size_x)/2);
			for(j=size_y-((i*size_y/size_x)+1);j>0;j--)
			{
				LCD_Write_DATA(fch,fcl);
			}		
		}
	}	
	else if(deg == 90)
	{
		x1 = x - size_x/2;
		x2 = x + size_x/2;
		y0 = y;
		y1 = y - size_y/2;
		y2 = y + size_y/2;
		
		for(i=0;i<size_y;i++)
		{			
			setXY(x-(size_x*i/size_y)/2,y1+i,x+(size_x*i/size_y)/2,y1+i);
			for(j=0;j<((i*size_x/size_y)+1);j++)
			{
				LCD_Write_DATA(fch,fcl);
			}		
		}	
	}
	else if(deg == 270)
	{
		x1 = x - size_x/2;
		x2 = x + size_x/2;
		y0 = y;
		y1 = y - size_y/2;
		y2 = y + size_y/2;
		
		for(i=0;i<size_y;i++)
		{			
			setXY(x-(size_x*(size_y-i)/size_y)/2,y1+i,x+(size_x*(size_y-i)/size_y)/2,y1+i);
			for(j=size_x-((i*size_x/size_y)+1);j>0;j--)
			{
				LCD_Write_DATA(fch,fcl);
			}		
		}
	}
	
	LCD_CS_PORT->BSRR = LCD_CS_PIN;
	clrXY();
}
/*
*	Ham chon font
* font: bang font chu muon su dung
*/
void setFont(uint8_t* font){
	cfont.font=font;
	cfont.x_size=font[0];
	cfont.y_size=font[1];
	cfont.offset=font[2];
	cfont.numchars=font[3];
}
