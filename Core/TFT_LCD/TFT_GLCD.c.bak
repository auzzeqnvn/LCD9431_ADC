void LCD_Reset(void){
	HAL_GPIO_WritePin(LCD_RST_PORT, LCD_RST_PIN, GPIO_PIN_SET);
	HAL_Delay(200);
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	HAL_Delay(200);
	HAL_GPIO_WritePin(LCD_RST_PORT, LCD_RST_PIN, GPIO_PIN_SET);	
} 

void LCD_DATA_Send16(unsigned int Data)
{
	unsigned int tmp = 0;
	
	if(Data & 0x8000)	tmp |= 0x0001;
	if(Data & 0x4000)	tmp |= 0x0002;
	if(Data & 0x2000)	tmp |= 0x0004;
	if(Data & 0x1000)	tmp |= 0x0008;
	if(Data & 0x0800)	tmp |= 0x0010;
	if(Data & 0x0400)	tmp |= 0x0020;
	if(Data & 0x0200)	tmp |= 0x0040;
	if(Data & 0x0100)	tmp |= 0x0080;
	if(Data & 0x0080)	tmp |= 0x0100;
	if(Data & 0x0040)	tmp |= 0x0200;
	if(Data & 0x0020)	tmp |= 0x0400;
	if(Data & 0x0010)	tmp |= 0x0800;
	if(Data & 0x0008)	tmp |= 0x1000;
	if(Data & 0x0004)	tmp |= 0x2000;
	if(Data & 0x0002)	tmp |= 0x4000;
	if(Data & 0x0001)	tmp |= 0x8000;
	
	GPIOD->ODR = tmp;	
}

void LCD_Write_COM(int da){  //LCD_WR_REG
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	LCD_RS_PORT->BSRR = (uint32_t)LCD_RS_PIN << 16U;
	LCD_DATA_Send16(da);
	LCD_CS_PORT->BSRR = LCD_CS_PIN;	
}
void LCD_Write_DATA(char VH,char VL){//LCD_WR_DATA
	unsigned int data;
	data = (VH<<8)|VL;
	LCD_RS_PORT->BSRR = LCD_RS_PIN;	
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	LCD_DATA_Send16(data);	
	LCD_CS_PORT->BSRR = LCD_CS_PIN;	
}
void LCD_WR_DATA(int da)//
{
	LCD_RS_PORT->BSRR = LCD_RS_PIN;	
	LCD_CS_PORT->BSRR = (uint32_t)LCD_CS_PIN << 16U;
	LCD_DATA_Send16(da);	
	LCD_CS_PORT->BSRR = LCD_CS_PIN;	
}
void LCD_Write_COM_DATA(char com1,int dat1){//LCD_WR_CMD
	LCD_Write_COM(com1);
	LCD_WR_DATA(dat1);
}

/*Enable LCD display*/
void LCD_Enable(void)
{
	HAL_GPIO_WritePin(LCD_RST_PORT, LCD_RST_PIN, GPIO_PIN_SET);
}

